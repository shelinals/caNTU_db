<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.root.cz3002.cantu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LauncherActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/app_name"
            android:name=".LauncherOwnerActivity"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:label="@string/app_name"
            android:name=".MainActivity"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:label="@string/app_name"
            android:name=".LoginActivity"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:label="Dabao Request"
            android:background="@color/pumpkin"
            android:name=".DabaoerActivity"
            android:theme="@style/AppTheme1"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:label="@string/app_name"
            android:name=".OwnerActivity"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:label="Credit"
            android:name=".Credit"
            android:background="@color/pumpkin"
            android:theme="@style/AppTheme1"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:label="Order List"
            android:name=".OrderActivity"
            android:background="@color/pumpkin"
            android:theme="@style/AppTheme1"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:label="Order Dabao Waiting List"
            android:name=".OrderListPayDabaoActivity"
            android:background="@color/pumpkin"
            android:theme="@style/AppTheme1"
            android:screenOrientation="sensorPortrait">
        </activity>


    </application>

</manifest>